input CreateUserInput {
  name: String
  email: String
}

api CreateUser {
  method: POST
  path: "/users"
  body: CreateUserInput
  response: User
  triggers: [UserCreated]
}

api Test {
  method: GET
  path: "/test"
  response: String
  triggers: [BottleneckDetected]
  middlewares: [auth, rate_limit, request_logging]
}


input WebSocketMessage {
  type: String
  payload: Json
}

